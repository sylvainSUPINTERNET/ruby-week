<h1>#<%= @paramHashtag %></h1>

<% @hashtags.each do |hashtag| %>

	<div class="card">
  <div class="card-block">
    <h4 class="card-title"><%= hashtag.title %></h4>
    <h6 class="card-subtitle mb-2 text-muted"><%= hashtag.updated_at.strftime('%F %T') %></h6>
    <p class="pull-right"><%= hashtag.user %></p>
    <p class="card-text"><%= hashtag.body %></p>
    <p class="card-link">#<%= hashtag.hashtag %></p>
  </div>
</div>
<hr>
<% end %>


